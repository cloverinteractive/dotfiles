FNM_PATH=`which fnm`
NIX_PROFILE=$HOME/.nix-profile/etc/profile.d/nix.sh
PYENV_PATH=`which pyenv`

if [[ -x $FNM_PATH ]]; then
  eval "`fnm env`"
fi

if [[ -x $PYENV_PATH ]]; then
  eval "$(pyenv init -)"
  eval "$(pyenv virtualenv-init -)"
fi

if [[ -f $NIX_PROFILE ]]; then
  source $NIX_PROFILE
fi

# vim: set ft=sh:
